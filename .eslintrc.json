{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "varsIgnorePattern": "^_", // _로 시작하는 변수 무시
        "argsIgnorePattern": "^_" // _로 시작하는 매개변수 무시
      }
    ]
  }
}
